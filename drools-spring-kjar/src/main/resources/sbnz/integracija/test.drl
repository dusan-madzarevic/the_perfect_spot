package sbnz.integracija;

import com.ftn.uns.ac.rs.theperfectmeal.model.User;


rule "Registered user existance"
	lock-on-active true
    when
        $ru: User(firstName == 'Dusan')
               
    then
    	
    	System.out.println("User exists in memory");
    	modify($ru){setFirstName('Ivana')};
end